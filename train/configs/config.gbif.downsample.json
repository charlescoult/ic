{ 
	"batch_size": 64,
	"max_epochs": 35,
	"model": {
		"base": "Xception",
		"classifier": {
			"dropout": 0.33,
			"output_normalize": false
		},
		"learning_rate": 0.001,
		"label_smoothing": 0.1
	},
	"dataset": {
		"data_augmentation": false,
		"downsample": 45,
		"source": "gbif",
		"split_test": 0.05,
		"split_val": 0.1

	},
	"callbacks": {
		"early_stopping": {
			"monitor": "val_loss",
			"patience": 10,
			"restore_best_weights": true,
			"start_from_epoch": 5
		}
	}
}
