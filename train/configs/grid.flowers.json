{
	"_run_group_type": "grid",
	"batch_size": [
		16,
		32,
		64,
		128
	],
	"max_epochs": 30,
	"model": {
		"base": [
			"Inception_v3_iNaturalist",
			"Xception"
		],
		"classifier": {
			"dropout": [
				0.0,
				0.3,
				0.5
			],
			"output_normalize": false
		},
		"learning_rate": [
			0.0001,
			0.0005,
			0.001
		],
		"label_smoothing": 0.1
	},
	"dataset": {
		"data_augmentation": false,
		"downsample": null,
		"source": "gbif",
		"split_test": 0.05,
		"split_val": 0.2

	},
	"callbacks": {
		"early_stopping": {
			"monitor": "val_loss",
			"patience": 8,
			"restore_best_weights": true,
			"start_from_epoch": 5
		}
	}
}
